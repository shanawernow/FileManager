<p id="notice"><%= notice %></p>

<h1>All Data</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>File size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.filename %></td>
        <td><%= datum.filesize %></td>
        <td><%= link_to 'Show', datum_path(datum) %></td>
        <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
        <td><%= link_to 'Destroy', datum_path(datum), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datum', new_user_datum_path %>
